{
  "name": "react-form-error",
  "version": "1.0.9",
  "description": "Simple React error handler for validation of form operations",
  "author": "github.com/atasoyfurkan",
  "license": "MIT",
  "repository": "github.com/atasoyfurkan/react-form-error",
  "homepage": "github.com/atasoyfurkan/react-form-error",
  "keywords": [
    "error",
    "handler",
    "form",
    "react",
    "validation",
    "joi",
    "bootstrap",
    "alert",
    "input"
  ],
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "microbundle-crl --no-compress --format modern,cjs",
    "start": "microbundle-crl watch --no-compress --format modern,cjs",
    "test": "run-s test:unit test:lint test:build",
    "test:build": "run-s build",
    "test:lint": "eslint .",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "predeploy": "cd example && npm run build",
    "deploy": "gh-pages -d example/build"
  },
  "peerDependencies": {
    "react": "^16.0.0",
    "bootstrap": "^4.4.1",
    "joi-browser": "^13.4.0"
  },
  "devDependencies": {
    "microbundle-crl": "^0.13.8",
    "cross-env": "^7.0.2",
    "gh-pages": "^2.2.0",
    "npm-run-all": "^4.1.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "^3.4.1",
    "bootstrap": "^4.4.1",
    "joi-browser": "^13.4.0"
  },
  "files": [
    "dist/index.js",
    "dist/index.modern.js"
  ],
  "dependencies": {
    "bootstrap": "^4.4.1",
    "joi-browser": "^13.4.0"
  }
}